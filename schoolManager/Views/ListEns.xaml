<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:viewModels="clr-namespace:schoolManager.ViewModels"
             x:Class="schoolManager.Views.ListEns"
             Title="Liste des Enseignants">    
    <ContentPage.BindingContext>
        <viewModels:ListEnsVM />
    </ContentPage.BindingContext>
    <StackLayout Spacing="10" VerticalOptions="CenterAndExpand" HorizontalOptions="CenterAndExpand">
        <Label Text="hueriohvioerho"/>
        <Label Text="{Binding Enss}" />
        <Label FontSize="22" FontAttributes="Bold" Text="{Binding Ens}" VerticalOptions="End" />
        <Button Text="Retour a la page Home" Clicked="Button_Home"/>
    </StackLayout>
</ContentPage>
